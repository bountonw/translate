<trans-unit id="34nYnKPJBVBA5wip_dc5:40" m:confirmed="1" m:created-at="1697121708931" m:created-by="1040200" m:gross-score="0.0" m:level-edited="false" m:locked="false" m:modified-at="1703418423252" m:modified-by="1040200" m:para-id="12" m:score="0.0" m:trans-origin="mt" xml:space="preserve">
