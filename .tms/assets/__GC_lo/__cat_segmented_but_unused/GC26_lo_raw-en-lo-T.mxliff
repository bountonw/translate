<trans-unit id="UKnsyQxPnrjB5wip_dc6:78" m:confirmed="0" m:created-at="1697121715053" m:created-by="1040200" m:gross-score="0.0" m:level-edited="false" m:locked="false" m:modified-at="1697121715053" m:modified-by="1040200" m:para-id="15" m:score="0.0" m:trans-origin="mt" xml:space="preserve">
<source>“Why do not our great men understand this Sabbath question?</source>
