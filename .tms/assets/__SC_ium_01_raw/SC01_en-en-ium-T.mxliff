<trans-unit id="DoJLWZ711uvxlh093_dc5:34" m:confirmed="1" m:created-at="1694056178233" m:created-by="1040200" m:gross-score="0.0" m:level-edited="false" m:locked="false" m:modified-at="1694056392854" m:modified-by="1040200" m:para-id="6" m:score="0.0" m:trans-origin="null" xml:space="preserve">
